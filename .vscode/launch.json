    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch KafkaTransactionsEnrichmentStreamingJob",
            "request": "launch",
            "mainClass": "dev.ramottamado.java.flink.KafkaTransactionsEnrichmentStreamingJob",
            "args": "--offset-reset earliest --bootstrap-server localhost:9092 --source-topic-1 postgres.public.transactions --source-topic-2 postgres.public.customers --debug-result-stream true --environment development --checkpoint-path file:///tmp/checkpoints",
            "projectName": "flink-java"
        }
    ]
}